#include <iostream>
#include <iomanip>

using namespace std;

int main()
{
    int a, b, c;
    
    cin >> a  >> b  >> c;
    if ((a == b) && (a == c) && (b == c))
        cout << 3 << endl;
    else if ((a == b) || (a == c) || (b == c))
        cout << 2 << endl;
    else if ((a != b) && (a != c) && (b != c))
        cout << 0 << endl;

}
